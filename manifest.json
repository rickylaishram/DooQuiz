{
	"manifest_version": 2,
	"name": "DooQuiz",
	"description": "Always get the perfect score on QuizDoo",
	"version": "1.0",
	"permissions": [
		"http://quizdoo.com/"
	],
	"content_scripts": [
		{
			"matches": ["http://quizdoo.com/*", "https://quizdoo.com/"],
			"js": ["jquery.js", "run.js"],
			"run_at": "document_end"
		}
	]
}